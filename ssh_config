    # Notebook NSSF
    LocalForward 9804 127.0.0.1:9039
    # Master MySQL
    LocalForward 3313 127.0.0.1:3350
    # Slave MySQL
    LocalForward 3314 127.0.0.1:3351

    ExitOnForwardFailure yes

